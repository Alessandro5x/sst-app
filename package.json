    "@nestjs/microservices": "^11.0.7",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.7",
    "@nestjs/websockets": "^11.0.7",
    "@types/pg": "^8.11.11",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "class-transformer": "^0.5.1",
